<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.91" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChatClient">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="299.0" prefWidth="340.0">
         <children>
            <Button fx:id="send_button" layoutX="262.0" layoutY="265.0" mnemonicParsing="false" onAction="#sendAction" prefHeight="25.0" prefWidth="62.0" text="Send" />
            <TextArea fx:id="chat_area" layoutY="43.0" prefHeight="222.0" prefWidth="340.0" />
            <TextField fx:id="port_tf" layoutX="161.0" layoutY="10.0" prefHeight="25.0" prefWidth="62.0" />
            <TextField fx:id="tf_chat" layoutY="265.0" prefHeight="25.0" prefWidth="253.0" />
            <TextField fx:id="name_tf" layoutX="46.0" layoutY="10.0" prefHeight="25.0" prefWidth="62.0" />
            <Button fx:id="connect_button" layoutX="262.0" layoutY="10.0" mnemonicParsing="false" onAction="#connectAction" prefHeight="25.0" prefWidth="62.0" text="Connect" />
            <Label layoutX="14.0" layoutY="14.0" text="Name" />
            <Label layoutX="127.0" layoutY="14.0" text="Port" />
